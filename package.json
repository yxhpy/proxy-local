{
  "name": "uvx-proxy-local",
  "version": "3.2.1",
  "main": "bin/index.js",
  "type": "module",
  "bin": {
    "uvx-proxy-local": "bin/index.js"
  },
  "scripts": {
    "test": "node test/run-tests.js",
    "test:provider-manager": "node test/provider-manager.test.js",
    "test:interface": "node test/interface-validation.test.js",
    "test:cli": "node test/cli-parser.test.js",
    "test:cloudflare": "node test/cloudflare-provider.test.js",
    "test:fallback": "node test/fallback-mechanism.test.js",
    "test:formatter": "node test/output-formatter.test.js",
    "test:config": "node test/config-loader.test.js",
    "start": "node bin/index.js",
    "release": "npm test && standard-version && git push --follow-tags origin master && npm publish",
    "release:dry": "npm test && standard-version --dry-run",
    "release:beta": "npm test && standard-version --prerelease beta && git push --follow-tags origin master && npm publish --tag beta"
  },
  "keywords": [
    "tunnel",
    "proxy",
    "localtunnel",
    "cli",
    "ngrok",
    "内网穿透"
  ],
  "author": "Your Name",
  "license": "MIT",
  "description": "多提供商内网穿透 CLI 工具，支持 Pinggy、Serveo、LocalTunnel 等免费服务，智能回退机制",
  "dependencies": {
    "@pinggy/pinggy": "^0.1.4",
    "chalk": "^5.6.0",
    "commander": "^14.0.0",
    "cosmiconfig": "^9.0.0",
    "inquirer": "^12.9.4",
    "localtunnel": "^2.0.2"
  },
  "devDependencies": {
    "standard-version": "^9.5.0"
  }
}
